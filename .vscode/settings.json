{
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/node_modules": true,
		"**/build": true
	},
	"files.watcherExclude": {
		"**/node_modules": true,
		"**/build": true
	}
}
